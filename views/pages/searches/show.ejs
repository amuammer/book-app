<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Search Results</title>
  </head>
  <body>
    <% books.forEach((book) => { %>
      <div>
        <img src="<%= book.image_url %>" alt="">
        <h1><%= book.title %></h1>
        <h1><%= book.author %></h1>
        <p><%= book.description %></p>
      </div>
    <% }); %>
  </body>
</html>
